<script>
    document.getElementById("menu-tabelas").className = 'active';
</script>
<table>
  <tbody>
    <% @lixodones.each do |lixodone| %>
      <tr>
        <td><%= lixodone.residente_id %></td>
        <td><%= lixodone.data %></td>
        <td><%= link_to 'Show', lixodone %></td>
        <td><%= link_to 'Edit', edit_lixodone_path(lixodone) %></td>
        <td><%= link_to 'Destroy', lixodone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
